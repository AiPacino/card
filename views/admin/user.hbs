
     <section id="main-content">
          <section class="wrapper">
              <!-- page start-->
              <div class="row">
                  <div class="col-lg-12">
                      <section class="col-md-12 panel panel-template " >
                          <header class="panel-heading">
                              用户管理
                          </header>
                          <div class="panel-body">
                              <div class="pro-sort">
                                  <label class="pro-lab">排序</label>
                                  <select class="styled" >
                                      <option>默认 &nbsp</option>
                                      <option>注册时间 &nbsp</option>
                                      <option>最近登录时间 &nbsp</option>
                                  </select>
                              </div>
                              <div class="pro-sort">
                                  <label class="pro-lab">搜索</label>
                                  <input type="text" class="search-input" placeholder="请输入关键词">
                                  <button type="button" class="btn btn-info"><i class="fa fa-search"></i></button>
                              </div>

                          </div>
                      </section>
                      <section class="panel">
                          
                          <table class="table table-striped table-advance table-hover">
                              <thead>
                              <tr>
                                  <th>用户名</th>
                                  <th>邮箱</th>
                                  <th>注册时间</th>
                                  <th>最近登录时间</th>
                                  <th>操作</th>
                              </tr>
                              </thead>
                              <tbody>
                              {{#each users}}
                                  <tr>
                                      <td><a href="#">{{username}}</a></td>
                                      <td class="hidden-phone">{{email}}</td>
                                      <td>{{meta.createAt}}</td>
                                      <td>{{meta.updateAt}}</td>
                                      <td>
                                          <button class="btn btn-danger btn-xs"><i class="fa fa-trash "></i></button>
                                      </td>
                                  </tr>
                              {{/each}}
             
                              </tbody>
                          </table>
                          
                      </section>

                  </div>
              </div>
              <!-- page end-->
          </section>
      </section>
      <!--main content end-->
      <!--footer start-->
      <!-- js placed at the end of the document so the pages load faster -->
    {{#section 'script'}}
    
    <!-- js placed at the end of the document so the pages load faster -->

    <script src="js/jquery.customSelect.min.js" ></script>
    <script type="text/javascript">
        
        $(function(){
            $('select.styled').customSelect();
        });
        
    </script>
    {{/section}}
